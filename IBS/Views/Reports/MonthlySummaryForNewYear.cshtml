@model IBS.Controllers.SearchFiltrsResult
@{
    ViewBag.Title = "Index";
}
<div class="row col-md-12">
    <h2>
        New Year Monthly Summary
    </h2>
    <hr />
</div>
<div class="row col-md-12 form-group">
    <div class="col-md-6">
        <label>Year:</label><span class="hash-redcolor">*</span>
        <select id="ddlYear" class="ibs-input ibs-border">
            <option value="0">Select</option>
            <option value="2020">2020</option>
        </select>
    </div>
    <div class="col-md-6">
        <label>Till Month:</label><span class="hash-redcolor">*</span>
        <select id="ddlMonth" class="ibs-input ibs-border">
            <option value="0">Select</option>
            <option value="1">January</option>
            <option value="2">February</option>
            <option value="3">March</option>
            <option value="4">April</option>
            <option value="5">May</option>
            <option value="6">June</option>
            <option value="7">July</option>
            <option value="8">August</option>
            <option value="9">September</option>
            <option value="10">October</option>
            <option value="11">November</option>
            <option value="12">December</option>
        </select>
    </div>
</div>
<div class="row col-md-12 form-group">
    <div class="col-md-6">
        <label>Coverages:</label><br />
        <select class="form-control ddlcoverages" multiple="multiple"></select>
    </div>
    <div class="col-md-6">
        <label>Product Type:</label><br />
        <select class="form-control ddlproducts" multiple="multiple"></select>
    </div>
</div>
<div class="row col-md-12 form-group">
    <div class="col-md-6">
        <label>Divisions:</label><br />
        <select class="form-control ddldivisions" multiple="multiple"></select>
    </div>
    <div class="col-md-6">
        <label>GIT:</label><br />
        <select class="form-control ddlGit" multiple="multiple">
            <option value="0">All</option>
            <option value="1">Yes</option>
            <option value="2">No</option>
        </select>
    </div>
</div>
<div class="col-md-12 form-group">
    <div class="col-md-4">
        <input id="btnSearch" type="submit" value="View Report" class="" />
    </div>
    <div class="col-md-4" style="text-align:right">
        <i class="fa fa-save" title="Save as Excel" id="btn_excel" aria-hidden="true" style="font-size:35px;"></i>
    </div>
    <div class="col-md-4" style="text-align:right">
        <i href="javascript:void(0)" title="Print" class="fa fa-print" aria-hidden="true" style="font-size:35px;" id="iPrint"></i>
