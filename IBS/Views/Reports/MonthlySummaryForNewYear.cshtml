    function print() {

        var divData = document.getElementById('divData');
        var htmlToPrint = '' + '<style type="text/css">' +
            'table, table tr th, table tr td {' +
            'border: solid 0.5px #aaa999' +
            '}' +
            'table {' +
            'border - collapse: collapse;' +
            'border-spacing: 0;' +
            '}' +
            '</style>'
        htmlToPrint += divData.outerHTML;
        var printWindow = window.open(""/*'', '', 'height=400,width=400'*/);
        printWindow.document.write('<html><head><h2>Monthly Details</h2>');
        printWindow.document.write('</head><br/><br/><body>');
        var details = getHeader();
        var header = "<font style='font-size:10px;'>" + details + "</font>";
        printWindow.document.write(header);
        printWindow.document.write('<br/><br/>');
        printWindow.document.write(htmlToPrint);
        printWindow.document.write('</body></html>');
        printWindow.document.close();
        printWindow.print();
    }
    $("#btn_excel").click(function (e) {
        var divisions = $.map($(".ddldivisions option:selected"), function (val, i) { return $(val).text() });
        var gits = $.map($(".ddlGit option:selected"), function (val, i) { return $(val).text() });
        var headerHTML = "<table>"
            + "<tr><td colspan='3'><h2>Monthly Summary</h2></td></tr> "
            + "<tr><td>Year:</td><td align='left'>" + $("#ddlYear").val() + "</td></tr>"
            + "<tr><td>Month:</td><td align='left'>" + $("#ddlMonth").val() + "</td></tr>"
            + "<tr><td>Division:</td><td align='left'>" + divisions.join() + "</td></tr>"
            + "<tr><td>GIT:</td><td align='left'>" + gits.join() + "</td></tr>"
            + "</table><br/><br/>";
        var htmlBody = "";
        htmlBody += headerHTML;
        htmlBody += $('div[id$=divData]').html();      // to construct body
        tableToExcel(htmlBody, "MonthlySummaryFile")
    });
    function getHeader() {
        var divisions = $.map($(".ddldivisions option:selected"), function (val, i) { return $(val).text() });
        var gits = $.map($(".ddlGit option:selected"), function (val, i) { return $(val).text() });
        return "Year: " + $("#ddlYear").val() + "| Month: " + $("#ddlMonth").val() + "| Divisions: " + divisions.join() + " | " + "GIT: " + gits.join();;
    }

    var tableToExcel = (function () {
        var uri = 'data:application/vnd.ms-excel;base64,'
            , template = '<html xmlns:o="urn:schemas-microsoft-com:office:office" xmlns:x="urn:schemas-microsoft-com:office:excel" xmlns="http://www.w3.org/TR/REC-html40"><head><!--[if gte mso 9]><xml><x:ExcelWorkbook><x:ExcelWorksheets><x:ExcelWorksheet><x:Name>{worksheet}</x:Name><x:WorksheetOptions><x:DisplayGridlines/></x:WorksheetOptions></x:ExcelWorksheet></x:ExcelWorksheets></x:ExcelWorkbook></xml><![endif]--></head><body><table>{table}</table></body></html>'
            , base64 = function (s) { return window.btoa(unescape(encodeURIComponent(s))) }
            , format = function (s, c) { return s.replace(/{(\w+)}/g, function (m, p) { return c[p]; }) }
        return function (htmlData, name) {
            //table = $('div[id$=divData]');
            var ctx = { worksheet: name || 'Worksheet', table: htmlData }
            //window.location.href = uri + base64(format(template, ctx))
            var link = document.createElement("a");
            link.download = "MonthlySummaryReport_" + new Date().toISOString().replace(/[\-\:\.]/g, "") + ".xls";
            link.href = uri + base64(format(template, ctx));
            link.click();
            link.remove();
        }
    })()
