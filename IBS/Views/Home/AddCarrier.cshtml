@{
        ViewBag.Title = "Index";
    }



    <div class="row report-header">
        <h3>Add Carrier</h3>
    </div>
    <div class="row report-filters">
        <form method="get">
            <label>Carrier Name:</label>
            <input type="text" class="form-control" id="carrier-name">
            <div class="row" style="margin-top:2%">
                <div class="col-md-2">
                    <label>Status:</label>
                    <select class="form-control" id="carrier-status">
                        <option>Active</option>
                        <option>Not Active</option>
                    </select>
                </div>
            </div>
        </form>

    </div>

    <div class="row report-filters" style="margin-top:25px;">
        <input id="Submit" class="btn btn-primary" type="submit" value="Submit" />
    </div>


<div class="form-group">
    <div class="col-md-offset-2 col-md-10" style="color:green">
        @ViewBag.AlertMsg
    </div>
</div>
